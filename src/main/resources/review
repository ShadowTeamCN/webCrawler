                <div class="donate-money-cursor _hidden"></div>
            </div>
            <div class="donate-money-list">
                {% for idx, money in state.moneySlot.slice(0, 4) %}
                <div class="donate-money {{ ((state.order && money === state.order.money) || (!state.order && +idx === 2)) ? '_selected _cur' : '' }}" data-value="{{ money }}">
                    <div class="donate-money-circle">
                        <span class="donate-money-number">{{ money }}</span>
                        <span class="donate-money-unit">元</span>
                    </div>
                    <div class="donate-money-text">
                        {{ money }} 元
                    </div>
                    <div class="donate-money-msg-box">无</div>
                </div>
                {% end %}
                <div
                    class="donate-money {{ (state.order && state.moneySlot.indexOf(state.order.money) < 0) ? '_selected' : '' }}"
                    data-value="{{ (state.order && state.moneySlot.indexOf(state.order.money) < 0) ? state.order.money : '' }}"
                >
                    <div class="donate-money-circle">
                        <span class="donate-money-number">?</span>
                    </div>
                    <div class="donate-money-text">
                        <input autocomplete="off" type="text"
                               name="custom-donate-money"
                               placeholder="填写金额"
                               value="{{ (state.order && state.moneySlot.indexOf(state.order.money) < 0) ? state.order.money : '' }}"
                        >
                    </div>
                    <div class="donate-money-msg-box">无</div>
                </div>
            </div>
        </div>
        <div class="donate-comment">
            <textarea name="comment" cols="60" type="text" placeholder="给作者捎句话" autocomplete="off" rows="1">{{ (state.order && state.order.comment) ? state.order.comment.trim() : '' }}</textarea>
        </div>
        <div class="donate-action">
            <a class="donate-btn" href="javascript:;">好，赏了</a>
        </div>
        <div class="donate-bottom">
                <span class="tips">赞赏行为不会影响评论的排序</span>
            <label class="donate-sync" {{ (!state.can_post_status) ? 'disabled' : '' }}>
                <input name="sync" type="checkbox" value="1" {{ (!state.can_post_status) ? 'disabled' : '' }} {{ ((!state.order || state.order.sync) && state.can_post_status) ? 'checked' : '' }} /> 同步到豆瓣广播
            </label>
        </div>
    </div>
</script>

